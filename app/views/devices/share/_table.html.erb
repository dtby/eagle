<!-- 通用表格复用模块（UPS系统）,单列（key: value）-->
<table class="table table-bordered">
  <thead>
    <tr>
      <th colspan="2" class="text-center"><%= title %></th>
    </tr>
  </thead>
  <tbody>
    <% points.each do |k, v| %>
      <tr>
        <td width="60%" align="center"><b><%= k %></b></td>
        <td align="center"><%= v %></td>
      </tr>
    <% end %>
  </tbody>
</table>
